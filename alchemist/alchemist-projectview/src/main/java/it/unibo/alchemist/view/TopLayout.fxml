<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="100.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unibo.alchemist.controller.TopLayoutController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="btnNew" mnemonicParsing="false" styleClass="btnMenu" />
            <Button fx:id="btnOpen" mnemonicParsing="false" styleClass="btnMenu" />
            <Separator orientation="VERTICAL" prefHeight="200.0">
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </Separator>
            <Button fx:id="btnSave" mnemonicParsing="false" styleClass="btnMenu" />
            <Button fx:id="btnSaveAs" mnemonicParsing="false" styleClass="btnMenu" />
            <Separator orientation="VERTICAL" prefHeight="200.0">
               <padding>
                  <Insets bottom="10.0" top="10.0" />
               </padding>
            </Separator>
            <Button fx:id="btnImport" mnemonicParsing="false" styleClass="btnMenu" text="Import" onAction="#clickImport"/>
         </children>
      </HBox>
   </children>
</AnchorPane>
