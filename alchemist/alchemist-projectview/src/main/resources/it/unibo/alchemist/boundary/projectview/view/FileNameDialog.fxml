<AnchorPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unibo.alchemist.boundary.projectview.controller.FileNameDialogController">
   <children>
      <GridPane layoutX="44.0" layoutY="30.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="25.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="62.5" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="12.5" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" percentHeight="50.0" prefHeight="70.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="fileName" />
            <TextField fx:id="tfNameFile" GridPane.columnIndex="1" />
            <Label fx:id="lbEx" GridPane.columnIndex="2" GridPane.halignment="RIGHT" />
         </children>
      </GridPane>
      <ButtonBar layoutX="149.0" layoutY="96.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0">
        <buttons>
          <Button mnemonicParsing="false" fx:id="btnOk" onAction="#clickOK" />
          <Button mnemonicParsing="false" fx:id="btnCancel" onAction="#clickCancel" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
