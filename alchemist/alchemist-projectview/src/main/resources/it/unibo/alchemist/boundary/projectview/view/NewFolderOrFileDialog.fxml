<AnchorPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unibo.alchemist.boundary.projectview.controller.NewFolderOrFileDialogController">
   <children>
      <GridPane layoutX="50.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="50.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="50.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" percentHeight="50.0" prefHeight="35.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" percentHeight="50.0" prefHeight="35.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="label" GridPane.columnSpan="2" />
            <TextField fx:id="textField" GridPane.columnSpan="2" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <ButtonBar layoutX="193.0" layoutY="146.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0">
        <buttons>
          <Button fx:id="okBtn" mnemonicParsing="false" onAction="#clickOk" />
          <Button fx:id="cancelBtn" mnemonicParsing="false" onAction="#clickCancel" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
