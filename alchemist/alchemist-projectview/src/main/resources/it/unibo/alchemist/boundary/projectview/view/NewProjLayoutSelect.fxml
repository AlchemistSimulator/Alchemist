<AnchorPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unibo.alchemist.boundary.projectview.controller.NewProjLayoutSelectController">
   <children>
      <GridPane layoutX="23.0" layoutY="60.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="100.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" percentHeight="50.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" percentHeight="50.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="select" />
            <ChoiceBox fx:id="choiceTempl" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <ButtonBar layoutX="186.0" layoutY="240.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0">
        <buttons>
          <Button fx:id="backBtn" mnemonicParsing="false" onAction="#clickBack"/>
          <Button fx:id="finishBtn" mnemonicParsing="false" onAction="#clickFinish"/>
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
