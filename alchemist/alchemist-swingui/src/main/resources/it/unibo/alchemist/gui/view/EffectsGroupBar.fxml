<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <ButtonBar id="groupControlBar" fx:id="groupControlBar" buttonMinWidth="40.0" opacity="0.45" stylesheets="@ButtonsBarLayout.css" BorderPane.alignment="CENTER">
         <buttons>
            <JFXButton id="save" fx:id="save" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" mnemonicParsing="false" prefHeight="40.0" ripplerFill="WHITE" text="Save" textAlignment="CENTER" textFill="WHITE" wrapText="true" ButtonBar.buttonData="LEFT" />
            <JFXButton id="load" fx:id="load" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" minWidth="40.0" mnemonicParsing="false" prefHeight="40.0" ripplerFill="WHITE" stylesheets="" text="Load" textAlignment="CENTER" textFill="WHITE" wrapText="true" ButtonBar.buttonData="LEFT" />
            <JFXButton id="addGroup" fx:id="addGroup" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" minWidth="40.0" mnemonicParsing="false" prefHeight="40.0" ripplerFill="WHITE" stylesheets="" text="Add" textAlignment="CENTER" textFill="WHITE" wrapText="true" ButtonBar.buttonData="RIGHT" />
         </buttons>
      </ButtonBar>
   </bottom>
   <center>
      <ListView id="effectGroupsList" fx:id="effectGroupsList" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
